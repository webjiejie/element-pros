{
  "name": "element-pros",
  "version": "1.8.2",
  "main": "dist/index.umd.min.js",
  "author": "cyj",
  "description": "基于element-ui封装的vue2组件库",
  "keywords": [
    "element",
    "vue2",
    "element-ui",
    "element-pro",
    "element-pros"
  ],
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "serve": "vue-cli-service serve --no-lint",
    "build": "vue-cli-service build",
    "build:pro": "cross-env VUE_APP_MY_NEWEST=true vue-cli-service build",
    "lint": "vue-cli-service lint",
    "publish:dev": "cross-env isPublish=true vue-cli-service build --target lib --name index packages/index.js",
    "publish:pro": "cross-env isPublish=true vue-cli-service build --target lib --name index packages/index.js && npm publish"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0",
    "vue-router": "^3.5.1",
    "vue-template-compiler": "^2.6.14",
    "axios": "^1.7.7",
    "highlight.js": "^11.10.0",
    "element-ui": "^2.15.14",
    "vue": "^2.6.14",
    "vuex": "^4.1.0",
    "cross-env": "^7.0.3"
  }
}
